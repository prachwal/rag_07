      "syntaxKind": "modifier"
    },
    {
      "tagName": "@packageDocumentation",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@since",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@author",
      "syntaxKind": "modifier"
    },
    {
      "tagName": "@version",
      "syntaxKind": "modifier"
    },
    {
