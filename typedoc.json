{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "public/docs/api",
  "gitRevision": "main",
  "gitRemote": "origin",
  "tsconfig": "./tsconfig.json",
  "exclude": [
    "**/__pycache__/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/*.test.ts",
    "**/*.test.js",
    "**/*.spec.ts",
    "**/*.spec.js"
  ],
  "includeVersion": true,
  "sort": ["source-order"],
  "categorizeByGroup": false,
  "defaultCategory": "Other",
  "categoryOrder": [
    "Core",
    "Providers",
    "Services",
    "Utils",
    "Models",
    "CLI",
    "Other"
  ],
  "readme": "README.md",
  "theme": "default",
  "hideGenerator": false,
  "searchInComments": true,
  "plugin": ["typedoc-plugin-markdown"]
}
